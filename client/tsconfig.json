{

  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "module": "ES2020",
    "esModuleInterop": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "checkJs": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules", "build"
  ],
  "include": [
    "./**/*.js"
  ]
}
